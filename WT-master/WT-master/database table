create table signup(
user_name not null PRIMARY KEY int AUTO_INCREMENT,
user_first not null varchar(20),
user_last not null varchar(20),
user_email not null varchar(20),
user_password not null varchar(256),
);

create table shows(
sh_id not null PRIMARY KEY int AUTO_INCREMENT,
sh_name not null varchar(256),
sh_type not null varchar(256),
sh_dur not null varchar(256),
sh_release not null varchar(256),
sh_cast not null varchar(256),
sh_awards not null varchar(256),
);


